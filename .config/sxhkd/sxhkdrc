super + Return
	$TERMINAL
super + d
	dmenu_run
super + b
	$SCRIPTS/bookmarks_dmenu.sh $FILE
super + shift + b
	$SCRIPTS/bookmarks_dmenu.sh $TERMINAL
super + u
	$SCRIPTS/mountdrives.sh
super + shift + u
	$SCRIPTS/umountdrives.sh
super + shift + s
	pavucontrol
super + shift + a
	arandr
super + shift + p
	$SCRIPTS/displays.sh
super + a
	$SCRIPTS/primaryactions.sh
super + c
	galculator
Print
	$SCRIPTS/screenshot.sh f &
shift + Print
	$SCRIPTS/screenshot.sh a &
control + Print
	$SCRIPTS/screenshot.sh fc &
control + shift + Print
	$SCRIPTS/screenshot.sh ac &
super + l
	$SCRIPTS/lock.sh
super + shift + l
	$SCRIPTS/lock.sh full
super + shift + q
	$SCRIPTS/poweroff.sh
super + w
	$BROWSER
super + shift + w
	$BROWSER --private
super + x
	$FILE
super + m
	audacious -m
super + shift + m
	multimc
XF86AudioPlay
	audacious -t
XF86AudioPrev
	audacious -r
XF86AudioNext
	audacious -f
shift + XF86AudioPlay
	audacious -j
XF86AudioRaiseVolume
	$SCRIPTS/volume.sh inc
XF86AudioLowerVolume
	$SCRIPTS/volume.sh dec
XF86AudioMute
	$SCRIPTS/volume.sh mute
XF86AudioMicMute
	$SCRIPTS/volume.sh mutemic
