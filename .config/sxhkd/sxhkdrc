super + Return
	$TERMINAL
super + d
	dmenu_run
super + b
	$SCRIPTS/bookmarks_dmenu.sh
super + u
	$SCRIPTS/mountdrives.sh
super + shift + u
	$SCRIPTS/umountdrives.sh
super + shift + s
	pavucontrol
super + shift + a
	arandr
super + p
	import -window root "$HOME/pics/screens/root_$(date +%s).png"
super + shift + p
	import "$HOME/pics/screens/area_$(date +%s).png"
super + F1
	i3lock --color '#000000'
super + shift + q
	$SCRIPTS/poweroff.sh
super + w
	$BROWSER
super + shift + w
	$BROWSER --private
super + x
	$FILE
XF86AudioPlay
	audacious -t
XF86AudioPrev
	audacious -r
XF86AudioNext
	audacious -f
shift + XF86AudioPlay
	audacious -j
